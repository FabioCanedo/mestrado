	number = {4},
	urldate = {2018-02-16},
	journal = {The European Physical Journal C},
	author = {H1 Collaboration},
	month = aug,
	year = {2003},
	note = {arXiv: hep-ex/0302034},
	keywords = {High Energy Physics - Experiment},
	pages = {497--513},
	file = {arXiv\:hep-ex/0302034 PDF:files/75/H1 Collaboration - 2003 - Measurement of inclusive jet cross sections in pho.pdf:application/pdf;arXiv.org Snapshot:files/78/0302034.html:text/html}
}

