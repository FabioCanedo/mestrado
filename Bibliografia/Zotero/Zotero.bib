@article{andrade_jet_2014,
	title = {Jet quenching effects on the anisotropic flow at {RHIC}},
	volume = {932},
	issn = {03759474},
	url = {http://arxiv.org/abs/1405.0178},
	doi = {10.1016/j.nuclphysa.2014.07.021},
	abstract = {In this article we investigate how the energy and momentum deposited by partonic dijets in the quark-gluon plasma may perturb the geometry-induced hydrodynamic expansion of the bulk nuclear matter created in heavy ion collisions at RHIC. The coupling between the jets and the medium is done through a source term in the energy-momentum conservation equations for ideal hydrodynamics. We concentrate our attention at mid-rapidity and solve the equations event-by-event imposing boost-invariance. For \$p\_T {\textbackslash}gtrsim 1\$ GeV the anisotropic flow is found to be considerably enhanced, if the dijets deposit on average more than 12 GeV in the medium (or equivalently 6 GeV for each jet of the pair), which corresponds, in our model, to an average suppression greater than \$65{\textbackslash}\%\$ of the initial jet transverse energy.},
	urldate = {2018-02-06},
	journal = {Nuclear Physics A},
	author = {Andrade, Rone P. G. and Noronha, Jorge and Denicol, Gabriel S.},
	month = dec,
	year = {2014},
	note = {arXiv: 1405.0178},
	keywords = {High Energy Physics - Phenomenology, Nuclear Experiment, Nuclear Theory},
	pages = {432--436},
	file = {arXiv\:1405.0178 PDF:files/25/Andrade et al. - 2014 - Jet quenching effects on the anisotropic flow at R.pdf:application/pdf;arXiv.org Snapshot:files/26/1405.html:text/html}
}

@article{lokhtin_hydjet++_2009,
	title = {{HYDJET}++ heavy ion event generator and its applications for {RHIC} and {LHC}},
	volume = {2009},
	issn = {1029-8479},
	url = {http://arxiv.org/abs/0903.0525},
	doi = {10.1088/1126-6708/2009/09/023},
	abstract = {The heavy ion event generator HYDJET++ is presented. HYDJET++ simulates relativistic heavy ion AA collisions as a superposition of the soft, hydro-type state and the hard state resulting from multi-parton fragmentation. This model is the development and continuation of HYDJET event generator. The hard parts of HYDJET and HYDJET++ are identical. The soft part of HYDJET++ contains the following important additional features as compared with HYDJET: resonance decays and more detailed treatment of thermal and chemical freeze-out hypersurfaces. HYDJET++ is capable of reproducing the bulk properties of heavy ion collisions at RHIC (hadron spectra and ratios, radial and elliptic flow, femtoscopic momentum correlations), as well as high-pT hadron spectra. Some applications of HYDJET++ at LHC are discussed.},
	number = {09},
	urldate = {2018-02-06},
	journal = {Journal of High Energy Physics},
	author = {Lokhtin, I. P. and Malinina, L. V. and Petrushanko, S. V. and Snigirev, A. M. and Arsene, I. and Tywoniuk, K.},
	month = sep,
	year = {2009},
	note = {arXiv: 0903.0525},
	keywords = {High Energy Physics - Phenomenology},
	pages = {023--023},
	file = {arXiv\:0903.0525 PDF:files/29/Lokhtin et al. - 2009 - HYDJET++ heavy ion event generator and its applica.pdf:application/pdf;arXiv.org Snapshot:files/30/0903.html:text/html}
}

@article{nattrass_jet_2018,
	title = {Jet quenching: an iconic result revisited},
	shorttitle = {Jet quenching},
	url = {http://arxiv.org/abs/1801.09131},
	abstract = {Early measurements at the Relativistic Heavy Ion Collider (RHIC) demonstrated jet quenching through the suppression of pairs of high momentum hadrons. These dihadron correlations have a large correlated background. As our understanding improved, we realized that a significant term was omitted from the background and several dihadron correlation results were quantitatively and qualitatively incorrect. The original measurements demonstrating jet quenching have not been revisited. These measurements are repeated in this paper in a kinematic range similar to the original measurement using publicly available data, applying our current knowledge about the background. The new results are qualitatively consistent with the previous results, demonstrating complete suppression of the away-side within uncertainties.},
	urldate = {2018-02-06},
	journal = {arXiv:1801.09131 [nucl-ex]},
	author = {Nattrass, Christine},
	month = jan,
	year = {2018},
	note = {arXiv: 1801.09131},
	keywords = {Nuclear Experiment},
	file = {arXiv\:1801.09131 PDF:files/32/Nattrass - 2018 - Jet quenching an iconic result revisited.pdf:application/pdf;arXiv.org Snapshot:files/33/1801.html:text/html}
}

