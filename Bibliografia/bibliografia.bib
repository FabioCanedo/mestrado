@article{andrade_jet_2014,
	title = {Jet quenching effects on the anisotropic flow at {RHIC}},
	volume = {932},
	issn = {03759474},
	url = {http://arxiv.org/abs/1405.0178},
	doi = {10.1016/j.nuclphysa.2014.07.021},
	abstract = {In this article we investigate how the energy and momentum deposited by partonic dijets in the quark-gluon plasma may perturb the geometry-induced hydrodynamic expansion of the bulk nuclear matter created in heavy ion collisions at RHIC. The coupling between the jets and the medium is done through a source term in the energy-momentum conservation equations for ideal hydrodynamics. We concentrate our attention at mid-rapidity and solve the equations event-by-event imposing boost-invariance. For \$p\_T {\textbackslash}gtrsim 1\$ GeV the anisotropic flow is found to be considerably enhanced, if the dijets deposit on average more than 12 GeV in the medium (or equivalently 6 GeV for each jet of the pair), which corresponds, in our model, to an average suppression greater than \$65{\textbackslash}\%\$ of the initial jet transverse energy.},
	urldate = {2018-02-06},
	journal = {Nuclear Physics A},
	author = {Andrade, Rone P. G. and Noronha, Jorge and Denicol, Gabriel S.},
	month = dec,
	year = {2014},
	note = {arXiv: 1405.0178},
	keywords = {High Energy Physics - Phenomenology, Nuclear Experiment, Nuclear Theory},
	pages = {432--436},
	annote = {Comment: 4 pages, 2 figures, Proceedings for the 6th International Conference on Hard and Electromagnetic Probes of High-Energy Nuclear Collisions (Hard Probes 2013)},
	file = {arXiv\:1405.0178 PDF:/home/fabio/Zotero/storage/M6L7J28F/Andrade et al. - 2014 - Jet quenching effects on the anisotropic flow at R.pdf:application/pdf;arXiv.org Snapshot:/home/fabio/Zotero/storage/XBZSRMQF/1405.html:text/html}
}

