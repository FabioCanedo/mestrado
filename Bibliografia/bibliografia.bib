	file = {ScienceDirect Full Text PDF:/home/fabio/Zotero/storage/BKW4PPGI/Bailhache - 2014 - Heavy-flavour elliptic flow measured in Pb–Pb coll.pdf:application/pdf;ScienceDirect Snapshot:/home/fabio/Zotero/storage/8C87B9W3/S0375947414003704.html:text/html}
}

@article{alice_collaboration_measurement_2017,
	title = {Measurement of the production of high-\$p\_\{{\textbackslash}rm {T}\}\$ electrons from heavy-flavour hadron decays in {Pb}-{Pb} collisions at \${\textbackslash}sqrt\{s\_\{{\textbackslash}rm {NN}\}\}\$ = 2.76 {TeV}},
	volume = {771},
	issn = {03702693},
	url = {http://arxiv.org/abs/1609.07104},
	doi = {10.1016/j.physletb.2017.05.060},
	abstract = {Electrons from heavy-flavour hadron decays (charm and beauty) were measured with the ALICE detector in Pb-Pb collisions at a centre-of-mass of energy \${\textbackslash}sqrt\{s\_\{{\textbackslash}rm NN\}\}=2.76\$ TeV. The transverse momentum (\$p\_\{{\textbackslash}rm T\}\$) differential production yields at mid-rapidity were used to calculate the nuclear modification factor \$R\_\{{\textbackslash}rm AA\}\$ in the interval \$3{\textless}p\_\{{\textbackslash}rm T\}{\textless} 18\$ GeV/\$c\$. The \$R\_\{{\textbackslash}rm AA\}\$ shows a strong suppression compared to binary scaling of pp collisions at the same energy (up to a factor of 4) in the 10\% most central Pb-Pb collisions. There is a centrality trend of suppression, and a weaker suppression (down to a factor of 2) in semi-peripheral (50-80\%) collisions is observed. The suppression of electrons in this broad \$p\_\{{\textbackslash}rm T\}\$ interval indicates that both charm and beauty quarks lose energy when they traverse the hot medium formed in Pb-Pb collisions at LHC.},
	urldate = {2018-03-05},
	journal = {Physics Letters B},
	author = {ALICE Collaboration},
	month = aug,
	year = {2017},
	note = {arXiv: 1609.07104},
	keywords = {Nuclear Experiment, High Energy Physics - Experiment},
	pages = {467--481},
	annote = {Comment: 28 pages, 7 captioned figures, 2 tables, authors from page 23, published version, figures at http://aliceinfo.cern.ch/ArtSubmission/node/3162},
	file = {arXiv\:1609.07104 PDF:/home/fabio/Zotero/storage/QZ8E9SP5/ALICE Collaboration - 2017 - Measurement of the production of high-\$p_ rm T \$ .pdf:application/pdf;arXiv.org Snapshot:/home/fabio/Zotero/storage/9JVUNWFC/1609.html:text/html}
}

@article{sakai_measurements_nodate,
