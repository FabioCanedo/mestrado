@article{andrade_jet_2014,
	title = {Jet quenching effects on the anisotropic flow at {RHIC}},
	volume = {932},
	issn = {03759474},
	url = {http://arxiv.org/abs/1405.0178},
	doi = {10.1016/j.nuclphysa.2014.07.021},
	abstract = {In this article we investigate how the energy and momentum deposited by partonic dijets in the quark-gluon plasma may perturb the geometry-induced hydrodynamic expansion of the bulk nuclear matter created in heavy ion collisions at RHIC. The coupling between the jets and the medium is done through a source term in the energy-momentum conservation equations for ideal hydrodynamics. We concentrate our attention at mid-rapidity and solve the equations event-by-event imposing boost-invariance. For \$p\_T {\textbackslash}gtrsim 1\$ GeV the anisotropic flow is found to be considerably enhanced, if the dijets deposit on average more than 12 GeV in the medium (or equivalently 6 GeV for each jet of the pair), which corresponds, in our model, to an average suppression greater than \$65{\textbackslash}\%\$ of the initial jet transverse energy.},
	urldate = {2018-04-04},
	journal = {Nuclear Physics A},
	author = {Andrade, Rone P. G. and Noronha, Jorge and Denicol, Gabriel S.},
	month = dec,
	year = {2014},
	note = {arXiv: 1405.0178},
	keywords = {High Energy Physics - Phenomenology, Nuclear Experiment, Nuclear Theory},
	pages = {432--436},
	annote = {Comment: 4 pages, 2 figures, Proceedings for the 6th International Conference on Hard and Electromagnetic Probes of High-Energy Nuclear Collisions (Hard Probes 2013)},
	file = {arXiv.org Snapshot:/home/fabio/Zotero/storage/DPASWQJM/1405.html:text/html;arXiv\:1405.0178 PDF:/home/fabio/Zotero/storage/ZR87EBX6/Andrade et al. - 2014 - Jet quenching effects on the anisotropic flow at R.pdf:application/pdf}
}

@article{prino_open_2016,
	title = {Open {Heavy} {Flavor} in {QCD} {Matter} and in {Nuclear} {Collisions}},
	volume = {43},
	issn = {0954-3899, 1361-6471},
	url = {http://arxiv.org/abs/1603.00529},
	doi = {10.1088/0954-3899/43/9/093002},
	abstract = {We review the experimental and theoretical status of open heavy-flavor (HF) production in high-energy nuclear collisions at RHIC and LHC. We first overview the theoretical concepts and pertinent calculations of HF transport in QCD matter, including perturbative and non-perturbative approaches in the quark-gluon plasma, effective models in hadronic matter, as well as implementations of heavy-quark (HQ) hadronization. This is followed by a brief discussion of bulk evolution models for heavy-ion collisions and initial conditions for the HQ distributions which are needed to calculate HF spectra in comparison to observables. We then turn to a discussion of experimental data that have been collected to date at RHIC and LHC, specifically for the nuclear suppression factor and elliptic flow of semileptonic HF decays, D mesons, non-prompt \$J/{\textbackslash}psi\$ from B-meson decays, and b-jets. Model comparisons to HF data are conducted with regards to extracting the magnitude, temperature and momentum-dependence of HF transport coefficients from experiment.},
	number = {9},
	urldate = {2018-04-03},
	journal = {Journal of Physics G: Nuclear and Particle Physics},
	author = {Prino, Francesco and Rapp, Ralf},
	month = sep,
	year = {2016},
	note = {arXiv: 1603.00529},
	keywords = {High Energy Physics - Phenomenology, Nuclear Experiment, Nuclear Theory, High Energy Physics - Experiment},
	pages = {093002},
	annote = {Comment: 89 pages, 30 figures, review article},
	file = {arXiv.org Snapshot:/home/fabio/Zotero/storage/DN6MQDTK/1603.html:text/html;arXiv\:1603.00529 PDF:/home/fabio/Zotero/storage/GYJUPBZD/Prino and Rapp - 2016 - Open Heavy Flavor in QCD Matter and in Nuclear Col.pdf:application/pdf}
}

@article{averbeck_heavy-flavor_2013,
	title = {Heavy-flavor production in heavy-ion collisions and implications for the properties of hot {QCD} matter},
	volume = {70},
	issn = {01466410},
	url = {http://arxiv.org/abs/1505.03828},
	doi = {10.1016/j.ppnp.2013.01.001},
	abstract = {Hadrons carrying open heavy flavor, i.e. single charm or bottom quarks, are among the key diagnostic tools available today for the hot and dense state of strongly interacting matter which is produced in collisions of heavy atomic nuclei at ultra-relativistic energies. First systematic heavy-flavor measurements in nucleus-nucleus collisions and the reference proton-proton system at Brookhaven National Laboratory's (BNL) Relativistic Heavy Ion Collider (RHIC) have led to tantalizing results. These studies are now continued and extended at RHIC and at CERN's Large Hadron Collider (LHC), where considerably higher collision energies are available. This review focuses on experimental results on open heavy-flavor observables at RHIC and the LHC published until July 2012. Yields of heavy-flavor hadrons and their decay products, their transverse momentum and rapidity distributions, as well as their azimuthal distributions with respect to the reaction plane in heavy-ion collisions are investigated. Various theoretical approaches are confronted with the data and implications for the properties of the hot and dense medium produced in ultra-relativistic heavy-ion collisions are discussed.},
	urldate = {2018-04-03},
	journal = {Progress in Particle and Nuclear Physics},
	author = {Averbeck, R.},
	month = may,
	year = {2013},
	note = {arXiv: 1505.03828},
	keywords = {Nuclear Experiment, High Energy Physics - Experiment},
	pages = {159--209},
	annote = {Comment: Review article, 78 pages, 38 figures},
	file = {arXiv.org Snapshot:/home/fabio/Zotero/storage/5US2IPWH/1505.html:text/html;arXiv\:1505.03828 PDF:/home/fabio/Zotero/storage/PEWYP4YK/Averbeck - 2013 - Heavy-flavor production in heavy-ion collisions an.pdf:application/pdf}
}

@article{andronic_heavy-flavour_2016,
