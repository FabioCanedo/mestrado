	keywords = {High Energy Physics - Phenomenology, High Energy Physics - Theory},
	annote = {Comment: 4+5 pages, 4 figures, 1 table. Version accepted for publication in PRD},
	file = {arXiv.org Snapshot:/home/fabio/Zotero/storage/LKM897L2/1502.html:text/html;arXiv\:1502.01719 PDF:/home/fabio/Zotero/storage/TSTWDV3E/Larkoski et al. - 2015 - Sudakov Safety in Perturbative QCD.pdf:application/pdf}
