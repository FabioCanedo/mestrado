@article{wiedemann_transverse_1999,
	title = {Transverse momentum dependence of the {Landau}-{Pomeranchuk}-{Migdal} effect},
	volume = {B560},
	doi = {10.1016/S0550-3213(99)00458-7},
	abstract = {We study the transverse momentum dependence of the Landau-Pomeranchuk-Migdal effect in QED, starting from the high energy expansion of the solution of the Dirac equation in the presence of an external field. The angular integrated energy loss formula differs from an earlier expression of Zakharov by taking finite kinematical boundaries into account. In an expansion in powers of the opacity of the medium, we derive explicit expressions for the radiation cross section associated with N=1, 2 and 3 scatterings. We verify the Bethe-Heitler and the factorization limit, and we calculate corrections to the factorization limit proportional to the square of the target size. A closed form expression valid to arbitrary orders in the opacity is derived in the dipole approximation. The resulting radiation spectrum is non-analytic in the coupling constant which is traced back to the transverse momentum broadening of a hard parton undergoing multiple small angle Moliere scattering. In extending the results to QCD, we test a previously used dipole prescription by comparing to direct pQCD results for N=1 and 2. For N=1, the QCD dipole prescription reproduces exactly the Bertsch-Gunion radiation spectrum. For N=2, we find a sizeable correction which reduces to a multiplicative factor 17/8 at large separation.},
	journal = {Nucl.Phys.},
	author = {Wiedemann, Urs Achim and Gyulassy, Miklos},
	year = {1999},
	keywords = {quantum chromodynamics, energy loss, analytic properties, approximation: Bethe-Heitler, approximation: dipole, approximation: eikonal, differential cross section: approximation, Dirac equation: solution, electron: multiple scattering, external field, factorization, Landau-Pomeranchuk-Migdal effect, pacs: 25.75.+r, path integral, quantum electrodynamics, radiation, regularization, target: length, transverse momentum dependence},
	pages = {345--382},
	annote = {20 pages, Latex, 4 eps-figures, replaced by published version, minor typos corrected Journal-ref: Nucl. Phys. B560 (1999) 345-382},
	file = {Submitted Version:/home/fabio/Zotero/storage/VINK2RQJ/Wiedemann and Gyulassy - 1999 - Transverse momentum dependence of the Landau-Pomer.pdf:application/pdf;INSPIRE PDF Full Text:/home/fabio/Zotero/storage/3FKV848E/Wiedemann and Gyulassy - 1999 - Transverse momentum dependence of the Landau-Pomer.pdf:application/pdf`;INSPIRE Snapshot:/home/fabio/Zotero/storage/VPRG3T5Z/501243.html:text/html}
