	volume = {746},
	issn = {03702693},
	url = {http://arxiv.org/abs/1502.01689},
	doi = {10.1016/j.physletb.2015.04.039},
	abstract = {The transverse momentum (\$p\_\{{\textbackslash}rm T\}\$) spectrum and nuclear modification factor (\$R\_\{{\textbackslash}rm AA\}\$) of reconstructed jets in 0-10\% and 10-30\% central Pb-Pb collisions at \${\textbackslash}sqrt\{s\_\{{\textbackslash}rm NN\}\}=2.76\$ TeV were measured. Jets were reconstructed from charged and neutral particles, utilizing the ALICE tracking detectors and Electromagnetic Calorimeter (EMCal), with the anti-\$k\_\{{\textbackslash}rm T\}\$ jet algorithm with a resolution parameter of R=0.2. The jet \$p\_\{{\textbackslash}rm T\}\$ spectra are reported in the pseudorapidity interval of \${\textbar}\{{\textbackslash}eta\}\_\{{\textbackslash}rm jet\}{\textbar}{\textless}0.5\$ for \$40{\textless}p\_\{{\textbackslash}rm T,jet\}{\textless}120\$ GeV/\$c\$ in 0-10\% and for \$30{\textless}p\_\{{\textbackslash}rm T,jet\}{\textless}100\$ GeV/\$c\$ in 10-30\% collisions. Reconstructed jets were required to contain a leading charged particle with \$p\_\{{\textbackslash}rm T\}{\textgreater}5\$ GeV/\$c\$ to suppress jets constructed from the combinatorial background in Pb-Pb collisions. The effect of the leading charged particle requirement has been studied in both pp and Pb-Pb collisions and has been shown to have negligible effects on the \$R\_\{{\textbackslash}rm AA\}\$ within the uncertainties of the measurement. The nuclear modification factor is obtained by dividing the jet spectrum measured in Pb-Pb by that in pp collisions scaled by the number of independent nucleon-nucleon collisions estimated using a Glauber model. \$R\_\{{\textbackslash}rm AA\}\$ is found to be \$0.28{\textbackslash}pm0.04\$ in 0-10\% and \$0.35{\textbackslash}pm0.04\$ in 10-30\% collisions, independent of \$p\_\{{\textbackslash}rm T,jet\}\$ within the uncertainties of the measurement. The observed suppression is in fair agreement with expectations from two model calculations with different approaches to jet quenching.},
	urldate = {2020-01-23},
	journal = {Physics Letters B},
	author = {ALICE Collaboration},
	month = jun,
	year = {2015},
	note = {arXiv: 1502.01689},
	keywords = {Nuclear Experiment, High Energy Physics - Experiment},
	pages = {1--14},
	annote = {Comment: 27 pages, 5 captioned figures, 1 table, authors from page 21, published version, figures at http://aliceinfo.cern.ch/ArtSubmission/node/1197},
	file = {arXiv Fulltext PDF:/home/fabio/Zotero/storage/I2KQQ2ZC/ALICE Collaboration - 2015 - Measurement of jet suppression in central Pb-Pb co.pdf:application/pdf;arXiv.org Snapshot:/home/fabio/Zotero/storage/5A9UBGGT/1502.html:text/html}
}

@book{dissertori_quantum_2003,
	title = {Quantum {Chromodynamics}: {High} {Energy} {Experiments} and {Theory}},
	isbn = {978-0-19-850572-3},
	shorttitle = {Quantum {Chromodynamics}},
	abstract = {This book provides an introduction to Quantum Chromodynamics (QCD), the theory of strong interactions. It covers in full detail both the theoretical foundations and the experimental tests of the theory. Although the experimental chapters focus on recent measurements, the subject is placed intohistorical perspective by also summarizing the steps which led to the formulation of QCD. Measurements are discussed as they were performed by the LEP experiments at CERN, or at hadron-hadron and lepton-hadron colliders such as the TEVATRON at Fermilab and HERA at DESY. Emphasis is placed on highenergy tests of QCD, such as measurements of the strong coupling constant, investigations of the non-abelian structure of the underlying gauge group, determinations of nucleon structure functions, and studies of the non-perturbative hadronization process. This excellent text gives a detailedoverview of how QCD developed in the 20th century and where we stand with respect to a quantitative understanding after the turn of the millenium. The text is intended for graduate and postgraduate students as well as researchers, and includes numerous problems and solutions.},
	language = {en},
