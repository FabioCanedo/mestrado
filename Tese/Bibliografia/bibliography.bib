	author = {Skands, Peter},
	month = nov,
	year = {2013},
	note = {arXiv: 1207.2389},
	keywords = {High Energy Physics - Phenomenology, High Energy Physics - Theory},
	pages = {341--420},
	annote = {Comment: Lecture notes from a course given at TASI 2012. Last update: July 2017. 85 pages, including index at the back. arXiv admin note: text overlap with arXiv:1104.2863},
