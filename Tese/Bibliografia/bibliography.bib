
@article{noronha-hostler_bulk_2014,
	title = {Bulk viscosity-driven suppression of shear viscosity effects on the flow harmonics at energies available at the {BNL} {Relativistic} {Heavy} {Ion} {Collider}},
	volume = {90},
	url = {https://link.aps.org/doi/10.1103/PhysRevC.90.034907},
	doi = {10.1103/PhysRevC.90.034907},
	abstract = {The interplay between shear and bulk viscosities on the flow harmonics, vn's, at the BNL Relativistic Heavy Ion Collider is investigated using the newly developed relativistic 2+1 hydrodynamical code v-usphydro that includes bulk and shear viscosity effects both in the hydrodynamic evolution and at freeze-out. While shear viscosity is known to attenuate the flow harmonics, we find that the inclusion of bulk viscosity decreases the shear viscosity-induced suppression of the flow harmonics, bringing them closer to their values in ideal hydrodynamical calculations. Depending on the value of the bulk viscosity to entropy density ratio, ζ/s, in the quark-gluon plasma, the bulk viscosity-driven suppression of shear viscosity effects on the flow harmonics may require a reevaluation of the previous estimates of the shear viscosity to entropy density ratio, η/s, of the quark-gluon plasma previously extracted by comparing hydrodynamic calculations to heavy-ion data.},
	number = {3},
	urldate = {2018-04-04},
	journal = {Physical Review C},
	author = {Noronha-Hostler, Jacquelyn and Noronha, Jorge and Grassi, Frédérique},
	month = sep,
	year = {2014},
	pages = {034907},
	file = {APS Snapshot:/home/fabio/Zotero/storage/V8C6NXN5/PhysRevC.90.html:text/html;Full Text PDF:/home/fabio/Zotero/storage/E5NTQ4KH/Noronha-Hostler et al. - 2014 - Bulk viscosity-driven suppression of shear viscosi.pdf:application/pdf}
}

@article{noronha-hostler_event-by-event_2016,
	title = {Event-by-event hydrodynamics \$+\$ jet energy loss: {A} solution to the \${R}\_\{{AA}\} {\textbackslash}otimes v\_2\$ puzzle},
	volume = {116},
	issn = {0031-9007, 1079-7114},
	shorttitle = {Event-by-event hydrodynamics \$+\$ jet energy loss},
	url = {http://arxiv.org/abs/1602.03788},
	doi = {10.1103/PhysRevLett.116.252301},
	abstract = {High \$p\_T {\textgreater} 10\$ GeV elliptic flow, which is experimentally measured via the correlation between soft and hard hadrons, receives competing contributions from event-by-event fluctuations of the low \$p\_T\$ elliptic flow and event plane angle fluctuations in the soft sector. In this paper, a proper account of these event-by-event fluctuations in the soft sector, modeled via viscous hydrodynamics, is combined with a jet energy loss model to reveal that the positive contribution from low \$p\_T\$ \$v\_2\$ fluctuations overwhelms the negative contributions from event plane fluctuations. This leads to an enhancement of high \$p\_T {\textgreater} 10\$ GeV elliptic flow in comparison to previous calculations and provides a natural solution to the decade long high \$p\_T\$ \$R\_\{AA\} {\textbackslash}otimes v\_2\$ puzzle. We also present the first theoretical calculation of high \$p\_T\$ \$v\_3\$, which is shown to be compatible with current LHC data. Furthermore, we discuss how short wavelength jet-medium physics can be deconvoluted from the physics of soft, bulk event-by-event flow observables using event shape engineering techniques.},
	number = {25},
	urldate = {2018-04-04},
	journal = {Physical Review Letters},
	author = {Noronha-Hostler, Jacquelyn and Betz, Barbara and Noronha, Jorge and Gyulassy, Miklos},
	month = jun,
	year = {2016},
	note = {arXiv: 1602.03788},
	keywords = {High Energy Physics - Phenomenology, Nuclear Experiment, Nuclear Theory},
	annote = {Comment: 7 pages, 3 figures, references added, substantially revised, accepted for publication in Phys. Rev. Lett},
	file = {arXiv.org Snapshot:/home/fabio/Zotero/storage/J89K5H9M/1602.html:text/html;arXiv\:1602.03788 PDF:/home/fabio/Zotero/storage/UKW8EH7E/Noronha-Hostler et al. - 2016 - Event-by-event hydrodynamics \$+\$ jet energy loss .pdf:application/pdf}
