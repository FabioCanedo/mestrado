}

@article{schenke_fluctuating_2012,
	title = {Fluctuating {Glasma} {Initial} {Conditions} and {Flow} in {Heavy} {Ion} {Collisions}},
	volume = {108},
	url = {https://link.aps.org/doi/10.1103/PhysRevLett.108.252301},
	doi = {10.1103/PhysRevLett.108.252301},
	abstract = {We compute initial conditions in heavy ion collisions within the color glass condensate framework by combining the impact parameter dependent saturation model with the classical Yang-Mills description of initial Glasma fields. In addition to fluctuations of nucleon positions, this impact parameter dependent Glasma description includes quantum fluctuations of color charges on the length scale determined by the inverse nuclear saturation scale Qs. The model naturally produces initial energy fluctuations that are described by a negative binomial distribution. The ratio of triangularity to eccentricity ε3/ε2 is close to that in a model tuned to reproduce experimental flow data. We compare transverse momentum spectra and v2,3,4(pT) of pions from different models of initial conditions using relativistic viscous hydrodynamic evolution.},
	number = {25},
	urldate = {2020-01-09},
	journal = {Physical Review Letters},
	author = {Schenke, Björn and Tribedy, Prithwish and Venugopalan, Raju},
	month = jun,
	year = {2012},
	pages = {252301},
	file = {APS Snapshot:/home/fabio/Zotero/storage/44LN76YA/PhysRevLett.108.html:text/html;Accepted Version:/home/fabio/Zotero/storage/TSCMLJHR/Schenke et al. - 2012 - Fluctuating Glasma Initial Conditions and Flow in .pdf:application/pdf}
}

